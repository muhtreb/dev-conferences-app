<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for edition in editions %}
        <url>
            <loc>
                {{ url('edition_show', { slug: edition.slug }) }}
            </loc>
            <lastmod>{{ 'now'|date('Y-m-d') }}</lastmod>
        </url>
    {% endfor %}
</urlset>